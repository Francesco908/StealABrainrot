local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "üîß Admin Tools v4.0",
   LoadingTitle = "Loading advanced tools...",
   LoadingSubtitle = "Mobile Edition",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = "AdminToolsConfig",
      FileName = "MobileConfig"
   },
   KeySystem = true,
   KeySettings = {
      Title = "Admin Tools",
      Subtitle = "Key Required",
      Note = "Get key from Discord",
      FileName = "AdminKey",
      SaveKey = false,
      GrabKeyFromSite = false,
   }
})

-- Tab Main
local MainTab = Window:CreateTab("Main", 13014546637)
local MainSection = MainTab:CreateSection("Core Features")

-- Air Jump
local AirJumpToggle = MainTab:CreateToggle({
   Name = "ü¶ò Air Jump",
   CurrentValue = false,
   Flag = "AirJump",
   Callback = function(Value)
      AirJumpEnabled = Value
   end,
})

-- Speed Boost System
local SpeedBoostActive = false
local SpeedBoostToggle = MainTab:CreateToggle({
   Name = "üöÄ Speed Boost",
   CurrentValue = false,
   Flag = "SpeedBoost",
   Callback = function(Value)
      if Value then
         SpeedBoostButton = MainTab:CreateButton({
            Name = "Boost: OFF",
            Callback = function()
               SpeedBoostActive = not SpeedBoostActive
               SpeedBoostButton:Set("Boost: " .. (SpeedBoostActive and "ON" or "OFF"))
            end,
         })
         SpeedBoostButton:SetTextColor(SpeedBoostActive and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0))
      elseif SpeedBoostButton then
         SpeedBoostButton:Destroy()
         SpeedBoostButton = nil
         SpeedBoostActive = false
      end
   end,
})

-- Float Mode (Anti-Stun + Anti-AFK)
local FloatToggle = MainTab:CreateToggle({
   Name = "‚ú® Float Mode",
   CurrentValue = false,
   Flag = "FloatMode",
   Callback = function(Value)
      if Value then
         Rayfield:Notify({
            Title = "Float Mode Enabled",
            Content = "Stun immunity + Anti AFK active",
            Duration = 4,
            Image = 13014546637,
         })
      end
      FloatEnabled = Value
   end,
})

-- Steal Helper System
