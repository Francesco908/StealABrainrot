                char.HumanoidRootPart.CFrame = CFrame.new(skyPos)
                
                wait(0.5)
                
                -- Then go to base position (but keep the +256 height)
                local finalPos = Vector3.new(basePosition.X, basePosition.Y + 256, basePosition.Z)
                char.HumanoidRootPart.CFrame = CFrame.new(finalPos)
                
                wait(0.5)
                
                -- Finally go down to base
                char.HumanoidRootPart.CFrame = CFrame.new(basePosition)
                
                Rayfield:Notify({
                    Title = "Base Travel",
                    Content = "Traveled to base via sky route",
                    Duration = 2,
                    Image = 4483362458
                })
            end
        end)
    end)
end

