local flyEnabled = false
local flySpeed = 50
HelperTab:CreateToggle({
    Name = "✈️ Fly (Press E to toggle)",
    CurrentValue = false,
    Flag = "Fly",
    Callback = function(Value)
        flyEnabled = Value
        if Value then
            Rayfield:Notify({
                Title = "Fly Enabled",
                Content = "Press E to toggle flight mode",
                Duration = 5,
                Image = 6022668888
            })
        else
