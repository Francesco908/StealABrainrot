   Name = "üéØ Steal Helper",
   CurrentValue = false,
   Flag = "StealHelper",
   Callback = function(Value)
      StealHelperEnabled = Value
      if StealGUI then
         StealGUI.Enabled = Value
      end
      if Value then
         Rayfield:Notify({
            Title = "Steal Helper Active",
