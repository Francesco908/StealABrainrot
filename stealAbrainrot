    -- Window controls
    local minimizeBtn = createButton(header, 
        UDim2.new(0, 45, 0, 45), 
        UDim2.new(1, -100, 0.5, -22.5), 
        colors.warning, 
        "‚àí"
    )
    minimizeBtn.TextSize = 28
    
    local closeBtn = createButton(header, 
        UDim2.new(0, 45, 0, 45), 
        UDim2.new(1, -50, 0.5, -22.5), 
