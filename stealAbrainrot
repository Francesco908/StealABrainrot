local flyEnabled = false
local flySpeed = 50
local flying = false
local bodyVelocity

HelperTab:CreateToggle({
    Name = "‚úàÔ∏è Fly (Press E to toggle)",
    CurrentValue = false,
    Flag = "Fly",
    Callback = function(Value)
        flyEnabled = Value
        if not Value and bodyVelocity then
            bodyVelocity:Destroy()
            bodyVelocity = nil
            flying = false
        end
        
        if Value then
            Rayfield:Notify({
                Title = "Fly Enabled",
                Content = "Press E to toggle flight mode",
                Duration = 5,
                Image = 6022668888
            })
        else
            Rayfield:Notify({
                Title = "Fly Disabled",
                Content = "Flight mode deactivated",
                Duration = 3,
                Image = 6022668888
            })
        end
    end,
})

-- Fly system setup
local function setupFly()
    local player = game.Players.LocalPlayer
    local character = player.Character
    if not character or not character:FindFirstChild("HumanoidRootPart") then return end
    
    if bodyVelocity then
        bodyVelocity:Destroy()
    end
    
    bodyVelocity = Instance.new("BodyVelocity")
    bodyVelocity.Velocity = Vector3.new(0, 0, 0)
    bodyVelocity.MaxForce = Vector3.new(0, 0, 0)
    bodyVelocity.Parent = character.HumanoidRootPart
end

-- Initialize fly on character load
game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
    character:WaitForChild("HumanoidRootPart")
    if flyEnabled then
        setupFly()
    end
end)

-- ESP Tab
local ESPTab = Window:CreateTab("üëÅÔ∏è ESP", 6022668888)

local playersESP = {}
local espConnection

ESPTab:CreateToggle({
    Name = "üë§ Player ESP",
    CurrentValue = false,
    Flag = "PlayerESP",
    Callback = function(Value)
        if espConnection then
            espConnection:Disconnect()
            espConnection = nil
        end
        
        if Value then
            -- Create ESP for existing players
            for _, player in ipairs(game:GetService("Players"):GetPlayers()) do
                if player ~= game.Players.LocalPlayer then
                    createESP(player)
                end
            end
            
            -- Setup connections for new players
            espConnection = game:GetService("Players").PlayerAdded:Connect(function(player)
                createESP(player)
            end)
            
            Rayfield:Notify({
                Title = "Player ESP Enabled",
                Content = "Players highlighted in red",
                Duration = 3,
                Image = 6022668888
            })
        else
            -- Remove all ESP
            for player, highlight in pairs(playersESP) do
                if highlight then
                    highlight:Destroy()
                end
            end
            playersESP = {}
            
            Rayfield:Notify({
                Title = "Player ESP Disabled",
                Content = "Player highlights removed",
                Duration = 3,
                Image = 6022668888
            })
        end
    end,
})

function createESP(player)
    if playersESP[player] then return end
    
    local highlight = Instance.new("Highlight")
    highlight.Name = player.Name .. "_ESP"
    highlight.OutlineColor = Color3.new(1, 0, 0)
    highlight.FillColor = Color3.new(1, 0, 0)
    highlight.FillTransparency = 0.5
    playersESP[player] = highlight
    
    local function attachESP(character)
        if character then
            highlight.Parent = character
            highlight.Adornee = character
        end
    end
    
    if player.Character then
        attachESP(player.Character)
    end
    
    player.CharacterAdded:Connect(attachESP)
    player.CharacterRemoving:Connect(function()
        highlight.Adornee = nil
        highlight.Parent = nil
    end)
end

