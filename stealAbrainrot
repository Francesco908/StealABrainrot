-- Bottone Fast Steal
local fastStealButton = Instance.new("TextButton")
fastStealButton.Name = "FastStealButton"
fastStealButton.Size = UDim2.new(1, 0, 0, 40)
fastStealButton.Position = UDim2.new(0, 0, 0, 70)
fastStealButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
fastStealButton.Text = "🚀 FAST STEAL 🚀"
fastStealButton.TextColor3 = Color3.fromRGB(0, 0, 0)
fastStealButton.TextScaled = true
fastStealButton.Font = Enum.Font.GothamBold
fastStealButton.Parent = contentFrame

local buttonCorner = Instance.new("UICorner")
buttonCorner.CornerRadius = UDim.new(0, 8)
buttonCorner.Parent = fastStealButton

-- Variabili per il sistema di teletrasporto
local isMinimized = false
local isTeleporting = false
local teleportMethods = {
    "CFrame Teleport",
    "Humanoid MoveTo",
    "Root Part Position",
    "Character Position",
    "Velocity Boost",
    "Network Bypass"
}

-- Funzione per trovare Delivery Hitbox
local function findDeliveryHitbox()
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj.Name:lower():find("delivery") and obj.Name:lower():find("hitbox") then
            return obj
        elseif obj.Name:lower():find("delivery") and obj:IsA("Part") then
            return obj
        end
    end
    return nil
end

-- Funzione per ottenere spawn point
local function getSpawnPoint()
    local spawnLocation = workspace:FindFirstChild("SpawnLocation")
    if spawnLocation then
        return spawnLocation.Position + Vector3.new(0, 5, 0)
    end
    return Vector3.new(0, 50, 0)
end

-- Funzione per il teletrasporto
local function attemptTeleport(method, destination)
    local character = player.Character
    if not character then return false end
    
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    local humanoid = character:FindFirstChild("Humanoid")
    
    if not humanoidRootPart then return false end
    
    pcall(function()
        if method == "CFrame Teleport" then
            humanoidRootPart.CFrame = CFrame.new(destination)
        elseif method == "Humanoid MoveTo" and humanoid then
            humanoid:MoveTo(destination)
            humanoidRootPart.CFrame = CFrame.new(destination)
        elseif method == "Root Part Position" then
            humanoidRootPart.Position = destination
        elseif method == "Character Position" then
            character:MoveTo(destination)
        elseif method == "Velocity Boost" then
            local bodyVelocity = Instance.new("BodyVelocity")
            bodyVelocity.MaxForce = Vector3.new(4000, 4000, 4000)
            bodyVelocity.Velocity = (destination - humanoidRootPart.Position).Unit * 100
            bodyVelocity.Parent = humanoidRootPart
            wait(0.1)
            bodyVelocity:Destroy()
            humanoidRootPart.CFrame = CFrame.new(destination)
        elseif method == "Network Bypass" then
            humanoidRootPart.Anchored = true
            humanoidRootPart.CFrame = CFrame.new(destination)
            humanoidRootPart.Anchored = false
        end
    end)
    
    wait(0.1)
    return (humanoidRootPart.Position - destination).Magnitude < 10
end

-- Funzione principale Fast Steal
local function fastSteal()
    if isTeleporting then return end
    isTeleporting = true
    
    statusLabel.Text = "Searching for target... 🔍"
    statusLabel.TextColor3 = Color3.fromRGB(255, 165, 0)
    
    local deliveryHitbox = findDeliveryHitbox()
    local targetPosition
    
    if deliveryHitbox then
        targetPosition = deliveryHitbox.Position + Vector3.new(0, 5, 0)
        statusLabel.Text = "Delivery target found! 📦"
    else
        targetPosition = getSpawnPoint()
        statusLabel.Text = "Using spawn point 🏠"
    end
    
    wait(0.5)
    
    for i, method in ipairs(teleportMethods) do
        statusLabel.Text = "Trying method " .. i .. "/6: " .. method .. " ⚡"
        statusLabel.TextColor3 = Color3.fromRGB(255, 255, 0)
        
        -- Aggiorna progress bar
        local progress = i / #teleportMethods
        TweenService:Create(progressBar, TweenInfo.new(0.3), {Size = UDim2.new(progress, 0, 1, 0)}):Play()
        
        local success = attemptTeleport(method, targetPosition)
        
        if success then
            statusLabel.Text = "✅ TELEPORT SUCCESSFUL! ✅"
            statusLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
            
            -- Animazione di successo
            local successTween = TweenService:Create(fastStealButton, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, true), {
                BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            })
            successTween:Play()
            
            wait(2)
            break
        else
            wait(0.5)
        end
    end
    
    -- Reset dopo 3 secondi
    wait(3)
    statusLabel.Text = "Ready to steal! 🎯"
    statusLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
    TweenService:Create(progressBar, TweenInfo.new(0.3), {Size = UDim2.new(0, 0, 1, 0)}):Play()
    fastStealButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
    isTeleporting = false
end

-- Eventi dei bottoni
fastStealButton.MouseButton1Click:Connect(fastSteal)

closeButton.MouseButton1Click:Connect(function()
    screenGui:Destroy()
end)

minimizeButton.MouseButton1Click:Connect(function()
    isMinimized = not isMinimized
    if isMinimized then
        TweenService:Create(mainFrame, TweenInfo.new(0.3), {Size = UDim2.new(0, 300, 0, 30)}):Play()
        contentFrame.Visible = false
        minimizeButton.Text = "□"
    else
        TweenService:Create(mainFrame, TweenInfo.new(0.3), {Size = UDim2.new(0, 300, 0, 200)}):Play()
        contentFrame.Visible = true
        minimizeButton.Text = "_"
    end
end)

print("🔥 RUBBA UN BRAINROT - Fast Steal GUI loaded! 🔥")
