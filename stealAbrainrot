-- Variables
local isMinimized = false
local isTeleporting = false
local detectedMethod = nil
local teleportMethods = {
    {name = "CFrame Direct", func = "cframe", priority = 1},
    {name = "Root Position", func = "position", priority = 2},
    {name = "Humanoid MoveTo", func = "moveto", priority = 3},
    {name = "Character MoveTo", func = "charmove", priority = 4},
    {name = "Velocity Boost", func = "velocity", priority = 5},
    {name = "Network Bypass", func = "bypass", priority = 6},
    {name = "Anchor Teleport", func = "anchor", priority = 7},
    {name = "Force Teleport", func = "force", priority = 8}
}

-- Enhanced method detection with anti-teleport analysis
local function detectGameTeleportMethod()
    detectedMethodLabel.Text = "üîç ANALYZING GAME ARCHITECTURE..."
    
    local character = player.Character
    if not character then return nil end
    
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    if not humanoidRootPart then return nil end
    
    -- Store original position for safety
    originalPosition = humanoidRootPart.Position
    local testPosition = originalPosition + Vector3.new(0, 0.1, 0)
    
    -- First, check for anti-teleport systems
    detectedMethodLabel.Text = "üõ°Ô∏è SCANNING FOR ANTI-TELEPORT SYSTEMS..."
    optimalMethodLabel.Text = "üîç DETECTING SECURITY MEASURES..."
    
    local antiTeleportDetected = detectAntiTeleport()
    if antiTeleportDetected then
        optimalMethodLabel.Text = "‚ö†Ô∏è ANTI-TELEPORT DETECTED - PREPARING BYPASS"
    end
    
    wait(0.5)
    
    -- Test each method with bypass capabilities
    for i, method in ipairs(teleportMethods) do
        detectedMethodLabel.Text = "üîç TESTING METHOD " .. i .. "/8: " .. method.name:upper()
        
        local success = false
        local startTime = tick()
        local bypassSuccess = false
        
        pcall(function()
            -- Pre-test bypass setup
            for _, child in pairs(humanoidRootPart:GetChildren()) do
                if child:IsA("BodyPosition") or child:IsA("BodyVelocity") then
                    child:Destroy()
                end
            end
            
            if method.func == "cframe" then
                humanoidRootPart.Anchored = true
                humanoidRootPart.CFrame = CFrame.new(testPosition)
                humanoidRootPart.Anchored = false
                success = (humanoidRootPart.Position - testPosition).Magnitude < 0.5
            elseif method.func == "position" then
                humanoidRootPart.Anchored = true
                humanoidRootPart.Position = testPosition
                humanoidRootPart.Anchored = false
                success = (humanoidRootPart.Position - testPosition).Magnitude < 0.5
            elseif method.func == "anchor" then
                humanoidRootPart.Anchored = true
                humanoidRootPart.Position = testPosition
                wait(0.1)
                humanoidRootPart.Anchored = false
                success = (humanoidRootPart.Position - testPosition).Magnitude < 0.5
            end
            
            -- Test if position holds (anti-teleport bypass test)
            wait(0.2)
            if success and (humanoidRootPart.Position - testPosition).Magnitude < 1 then
                bypassSuccess = true
            end
        end)
        
        local responseTime = tick() - startTime
        
        if success and bypassSuccess and responseTime < 0.3 then
            detectedMethodLabel.Text = "‚úÖ OPTIMAL METHOD DETECTED: " .. method.name:upper()
            optimalMethodLabel.Text = "‚ö° BYPASS CAPABLE: " .. method.name:upper() .. " (PRIORITY " .. method.priority .. ")"
            
            -- Return to original position safely
            pcall(function()
                humanoidRootPart.CFrame = CFrame.new(originalPosition)
            end)
            
            return method
        end
        
        -- Return to original position after each test
        pcall(function()
            humanoidRootPart.CFrame = CFrame.new(originalPosition)
        end)
        
        wait(0.1)
    end
    
    detectedMethodLabel.Text = "‚ö†Ô∏è USING MAXIMUM BYPASS METHODS"
    optimalMethodLabel.Text = "üõ°Ô∏è FULL ANTI-TELEPORT COUNTERMEASURES ACTIVE"
    
    -- Ensure we're back at original position
    pcall(function()
        humanoidRootPart.CFrame = CFrame.new(originalPosition)
    end)
    
    return teleportMethods[1] -- Fallback to CFrame with maximum bypass
end

-- Anti-Teleport Detection & Bypass Variables
local antiTPActive = false
local originalPosition = nil
local teleportLocked = false
local bypassConnections = {}

-- Function to detect anti-teleport systems
local function detectAntiTeleport()
    local character = player.Character
    if not character then return false end
    
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    if not humanoidRootPart then return false end
    
    -- Check for common anti-teleport methods
    local detectionMethods = {
        "BodyPosition", "BodyVelocity", "BodyMovers", "PrimaryPart",
        "Changed", "Heartbeat", "RenderStepped", "RemoteEvents"
    }
    
    local detected = false
    
    -- Check for body movers that might reset position
    for _, child in pairs(humanoidRootPart:GetChildren()) do
        if child:IsA("BodyPosition") or child:IsA("BodyVelocity") or child:IsA("BodyAngularVelocity") then
            if not bypassConnections[child] then
                detected = true
            end
        end
    end
    
    return detected
end

-- Advanced Anti-Teleport Bypass System
local function setupAntiTeleportBypass()
    local character = player.Character
    if not character then return end
    
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    if not humanoidRootPart then return end
    
    -- Method 1: Disable all body movers that try to reset position
    local function neutralizeBodyMovers()
        for _, child in pairs(humanoidRootPart:GetChildren()) do
            if child:IsA("BodyPosition") or child:IsA("BodyVelocity") or child:IsA("BodyAngularVelocity") then
                if child.Name ~= "FastStealBypass" then
                    child:Destroy()
                end
            end
        end
    end
    
    -- Method 2: Network ownership bypass
    local function setNetworkOwnership()
        pcall(function()
            humanoidRootPart:SetNetworkOwner(player)
        end)
    end
    
    -- Method 3: Continuous position lock
    local function createPositionLock(targetPos)
        -- Remove any existing locks
        for name, connection in pairs(bypassConnections) do
            if connection then
                connection:Disconnect()
            end
        end
        bypassConnections = {}
        
        -- Create invisible anchor
        local lockPart = Instance.new("Part")
        lockPart.Name = "FastStealBypass"
        lockPart.Anchored = true
        lockPart.CanCollide = false
        lockPart.Transparency = 1
        lockPart.Size = Vector3.new(0.1, 0.1, 0.1)
        lockPart.CFrame = CFrame.new(targetPos)
        lockPart.Parent = workspace
        
        -- Weld to lock position
        local weld = Instance.new("WeldConstraint")
        weld.Part0 = humanoidRootPart
        weld.Part1 = lockPart
        weld.Parent = humanoidRootPart
        
        game:GetService("Debris"):AddItem(lockPart, 2)
        game:GetService("Debris"):AddItem(weld, 2)
    end
    
    -- Method 4: Heartbeat position enforcement
    local function enforcePosition(targetPos)
        bypassConnections["heartbeat"] = RunService.Heartbeat:Connect(function()
            if humanoidRootPart and humanoidRootPart.Parent then
                local currentPos = humanoidRootPart.Position
                if (currentPos - targetPos).Magnitude > 5 then
                    neutralizeBodyMovers()
                    humanoidRootPart.CFrame = CFrame.new(targetPos)
                    humanoidRootPart.Velocity = Vector3.new(0, 0, 0)
                    humanoidRootPart.RotVelocity = Vector3.new(0, 0, 0)
                end
            end
        end)
        
        -- Auto-disconnect after 3 seconds
        wait(3)
        if bypassConnections["heartbeat"] then
            bypassConnections["heartbeat"]:Disconnect()
            bypassConnections["heartbeat"] = nil
        end
    end
    
    -- Method 5: Character property override
    local function overrideCharacterProperties()
        pcall(function()
            if character.PrimaryPart then
                character.PrimaryPart = humanoidRootPart
            end
        end)
    end
    
    return neutralizeBodyMovers, setNetworkOwnership, createPositionLock, enforcePosition, overrideCharacterProperties
end

-- Enhanced teleport function with anti-teleport bypass
local function attemptTeleport(method, destination)
    local character = player.Character
    if not character then return false end
    
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    local humanoid = character:FindFirstChild("Humanoid")
    
    if not humanoidRootPart then return false end
    
    -- Setup anti-teleport bypass
    local neutralizeBodyMovers, setNetworkOwnership, createPositionLock, enforcePosition, overrideCharacterProperties = setupAntiTeleportBypass()
    
    -- Pre-teleport bypass setup
    neutralizeBodyMovers()
    setNetworkOwnership()
    overrideCharacterProperties()
    
    local success = false
    
    pcall(function()
        if method.func == "cframe" then
            humanoidRootPart.Anchored = true
            neutralizeBodyMovers()
            humanoidRootPart.CFrame = CFrame.new(destination)
            createPositionLock(destination)
            humanoidRootPart.Anchored = false
            
        elseif method.func == "position" then
            neutralizeBodyMovers()
            humanoidRootPart.Anchored = true
            humanoidRootPart.Position = destination
            humanoidRootPart.Anchored = false
            createPositionLock(destination)
            
        elseif method.func == "moveto" and humanoid then
            neutralizeBodyMovers()
            humanoid:MoveTo(destination)
            wait(0.1)
            humanoidRootPart.CFrame = CFrame.new(destination)
            createPositionLock(destination)
            
        elseif method.func == "charmove" then
            neutralizeBodyMovers()
            character:MoveTo(destination)
            wait(0.1)
            humanoidRootPart.CFrame = CFrame.new(destination)
            createPositionLock(destination)
            
        elseif method.func == "velocity" then
            neutralizeBodyMovers()
            local bodyVelocity = Instance.new("BodyVelocity")
            bodyVelocity.Name = "FastStealBypass"
            bodyVelocity.MaxForce = Vector3.new(4000, 4000, 4000)
            bodyVelocity.Velocity = (destination - humanoidRootPart.Position).Unit * 200
            bodyVelocity.Parent = humanoidRootPart
            wait(0.3)
            bodyVelocity:Destroy()
            humanoidRootPart.CFrame = CFrame.new(destination)
            createPositionLock(destination)
            
        elseif method.func == "bypass" then
            neutralizeBodyMovers()
            setNetworkOwnership()
            humanoidRootPart.Anchored = true
            humanoidRootPart.CFrame = CFrame.new(destination)
            wait(0.1)
            humanoidRootPart.Anchored = false
            createPositionLock(destination)
            
        elseif method.func == "anchor" then
            neutralizeBodyMovers()
            humanoidRootPart.Anchored = true
            humanoidRootPart.Position = destination
            wait(0.1)
            humanoidRootPart.Anchored = false
            createPositionLock(destination)
            
        elseif method.func == "force" then
            neutralizeBodyMovers()
            local bodyPosition = Instance.new("BodyPosition")
            bodyPosition.Name = "FastStealBypass"
            bodyPosition.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
            bodyPosition.Position = destination
            bodyPosition.P = 10000
            bodyPosition.D = 1000
            bodyPosition.Parent = humanoidRootPart
            wait(0.5)
            bodyPosition:Destroy()
            humanoidRootPart.CFrame = CFrame.new(destination)
            createPositionLock(destination)
        end
        
        success = true
    end)
    
    -- Post-teleport security measures
    if success then
        spawn(function()
            enforcePosition(destination)
        end)
        
        -- Additional security layer
        spawn(function()
            for i = 1, 10 do
                wait(0.2)
                if humanoidRootPart and (humanoidRootPart.Position - destination).Magnitude > 10 then
                    neutralizeBodyMovers()
                    humanoidRootPart.CFrame = CFrame.new(destination)
                    humanoidRootPart.Velocity = Vector3.new(0, 0, 0)
                end
            end
        end)
    end
    
    wait(0.2)
    return success and (humanoidRootPart.Position - destination).Magnitude < 20
end

-- Find delivery hitbox with enhanced search
local function findDeliveryHitbox()
    local searchTerms = {"delivery", "hitbox", "deliver", "drop", "endpoint", "target"}
    
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("BasePart") then
            local name = obj.Name:lower()
            for _, term in ipairs(searchTerms) do
                if name:find(term) then
                    return obj
                end
            end
        end
    end
    return nil
end

-- Get spawn point
local function getSpawnPoint()
    local spawnLocation = workspace:FindFirstChild("SpawnLocation")
    if spawnLocation then
        return spawnLocation.Position + Vector3.new(0, 5, 0)
    end
    
    -- Try to find other spawn points
    for _, obj in pairs(workspace:GetChildren()) do
        if obj.Name:lower():find("spawn") and obj:IsA("BasePart") then
            return obj.Position + Vector3.new(0, 5, 0)
        end
    end
    
    return Vector3.new(0, 50, 0)
end

-- Main fast steal function
local function executeFastSteal()
    if isTeleporting then return end
    isTeleporting = true
    
    fastStealButton.BackgroundColor3 = Color3.fromRGB(255, 193, 7)
    fastStealButton.Text = "‚è≥ EXECUTING..."
    
    -- Detect optimal method first
    statusLabel.Text = "üîç DETECTING OPTIMAL TELEPORT METHOD..."
    statusLabel.TextColor3 = Color3.fromRGB(255, 193, 7)
    
    local optimalMethod = detectGameTeleportMethod()
    wait(0.5)
    
    -- Find target
    statusLabel.Text = "üéØ LOCATING TARGET DESTINATION..."
    local deliveryHitbox = findDeliveryHitbox()
    local targetPosition
    
    if deliveryHitbox then
        targetPosition = deliveryHitbox.Position + Vector3.new(0, 5, 0)
        statusLabel.Text = "üì¶ DELIVERY HITBOX ACQUIRED!"
    else
        targetPosition = getSpawnPoint()
        statusLabel.Text = "üè† USING SPAWN COORDINATES"
    end
    
    wait(0.5)
    statusLabel.Text = "üõ°Ô∏è ACTIVATING ANTI-TELEPORT BYPASS..."
    statusLabel.TextColor3 = Color3.fromRGB(255, 69, 58)
    wait(0.3)
    
    statusLabel.Text = "‚ö° EXECUTING UNDETECTABLE TELEPORT..."
    statusLabel.TextColor3 = Color3.fromRGB(255, 255, 0)
    
    -- Try optimal method first, then fallbacks
    local methodsToTry = {optimalMethod}
    for _, method in ipairs(teleportMethods) do
        if method ~= optimalMethod then
            table.insert(methodsToTry, method)
        end
    end
    
    for i, method in ipairs(methodsToTry) do
        if i > 8 then break end -- Limit attempts
        
        statusLabel.Text = "‚ö° BYPASS METHOD " .. i .. "/8: " .. method.name:upper()
        
        -- Update progress
        local progress = i / math.min(8, #methodsToTry)
        TweenService:Create(progressBar, TweenInfo.new(0.3), {Size = UDim2.new(progress, 0, 1, 0)}):Play()
        progressText.Text = math.floor(progress * 100) .. "%"
        
        local success = attemptTeleport(method, targetPosition)
        
        if success then
            statusLabel.Text = "‚úÖ TELEPORT SUCCESSFUL! TARGET REACHED!"
            statusLabel.TextColor3 = Color3.fromRGB(0, 255, 127)
            
            -- Success animation
            TweenService:Create(fastStealButton, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, true), {
                BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            }):Play()
            
            -- Progress complete
            TweenService:Create(progressBar, TweenInfo.new(0.3), {Size = UDim2.new(1, 0, 1, 0)}):Play()
            progressText.Text = "100% - SUCCESS!"
            
            wait(3)
            break
        else
            wait(0.3)
        end
    end
    
    if (player.Character and player.Character:FindFirstChild("HumanoidRootPart") and 
        (player.Character.HumanoidRootPart.Position - targetPosition).Magnitude > 20) then
        statusLabel.Text = "‚ùå ALL METHODS FAILED - TRYING EMERGENCY PROTOCOLS"
        statusLabel.TextColor3 = Color3.fromRGB(255, 69, 58)
    end
    
    -- Reset after delay
    wait(3)
    statusLabel.Text = "üéØ READY TO EXECUTE FAST STEAL"
    statusLabel.TextColor3 = Color3.fromRGB(0, 255, 127)
    TweenService:Create(progressBar, TweenInfo.new(0.5), {Size = UDim2.new(0, 0, 1, 0)}):Play()
    progressText.Text = "0%"
    fastStealButton.BackgroundColor3 = Color3.fromRGB(0, 255, 127)
    fastStealButton.Text = "üöÄ EXECUTE FAST STEAL üöÄ"
    isTeleporting = false
end

-- Button events
fastStealButton.MouseButton1Click:Connect(executeFastSteal)

closeButton.MouseButton1Click:Connect(function()
    TweenService:Create(mainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back), {
        Size = UDim2.new(0, 0, 0, 0),
        Position = UDim2.new(0.5, 0, 0.5, 0)
    }):Play()
    wait(0.3)
    screenGui:Destroy()
end)

minimizeButton.MouseButton1Click:Connect(function()
    isMinimized = not isMinimized
    if isMinimized then
        TweenService:Create(mainFrame, TweenInfo.new(0.4, Enum.EasingStyle.Quart), {Size = UDim2.new(0, 380, 0, 40)}):Play()
        contentFrame.Visible = false
        minimizeButton.Text = "‚ñ°"
    else
        TweenService:Create(mainFrame, TweenInfo.new(0.4, Enum.EasingStyle.Quart), {Size = UDim2.new(0, 380, 0, 280)}):Play()
        contentFrame.Visible = true
        minimizeButton.Text = "‚àí"
    end
end)

-- Button hover effects
fastStealButton.MouseEnter:Connect(function()
    if not isTeleporting then
        TweenService:Create(fastStealButton, TweenInfo.new(0.2), {
            BackgroundColor3 = Color3.fromRGB(0, 200, 100),
            Size = UDim2.new(1, 5, 0, 52)
        }):Play()
    end
end)

fastStealButton.MouseLeave:Connect(function()
    if not isTeleporting then
        TweenService:Create(fastStealButton, TweenInfo.new(0.2), {
            BackgroundColor3 = Color3.fromRGB(0, 255, 127),
            Size = UDim2.new(1, 0, 0, 50)
        }):Play()
    end
end)

